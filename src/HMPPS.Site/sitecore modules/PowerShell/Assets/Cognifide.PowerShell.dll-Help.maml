          <maml:name>KeepSession</maml:name>
          <command:parameterValue required="true">SwitchParameter</command:parameterValue>
        </command:parameter>
        <command:parameter required="false" parameterSetName="__AllParameterSets" position="named">
          <maml:name>HostOutput</maml:name>
          <command:parameterValue required="true">SwitchParameter</command:parameterValue>
        </command:parameter>
      </command:syntaxItem>
      <command:syntaxItem>
        <maml:name>Receive-ScriptSession</maml:name>
        <command:parameter required="true" parameterSetName="Session" position="named">
          <maml:name>Session</maml:name>
