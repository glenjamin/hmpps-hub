      </command:example>
      <command:example>
        <maml:title>------------------EXAMPLE 2-----------------------</maml:title>
        <dev:code>$root = (Get-Item "master:/system/Modules/PowerShell/Script Library/")
Find-Item -Index sitecore_master_index `
          -Criteria @{Filter = "DescendantOf"; Field = $root } |
    Initialize-Item

</dev:code>
        <dev:remarks>Find all children of a specific item including that item - return Sitecore items</dev:remarks>
      </command:example>
      <command:example>
