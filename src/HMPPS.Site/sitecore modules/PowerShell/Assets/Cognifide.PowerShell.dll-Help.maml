    <command:details>
      <command:name>Export-Item</command:name>
      <gl:group>Item</gl:group>
      <maml:description>
        <maml:para>Exports (serializes) the Sitecore item to the filesystem.

</maml:para>
      </maml:description>
      <maml:copyright>
        <maml:para>© 2010-2017 Adam Najmanowicz, Michael West. All rights reserved. Sitecore PowerShell Extensions</maml:para>
