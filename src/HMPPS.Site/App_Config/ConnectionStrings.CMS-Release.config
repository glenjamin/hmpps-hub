<?xml version="1.0" encoding="utf-8" ?>
<!-- For more information on using transformations 
     see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<connectionStrings xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <add name="core" connectionString="#{sqlCore}#" xdt:Transform="Replace"/>
  <add name="master" connectionString="#{sqlMaster}#" xdt:Transform="Replace"/>
  <add name="web" connectionString="#{sqlWeb}#" />
  <add name="analytics" connectionString="#{mongoAnalytics}#" xdt:Transform="Replace"/>
  <add name="tracking.live" connectionString="#{mongoLive}#" xdt:Transform="Replace"/>
  <add name="tracking.contact" connectionString="#{mongoContact}#" xdt:Transform="Replace"/>
  <add name="reporting.apikey" connectionString="#{reportingKey}#" xdt:Transform="Replace"/>
  <add name="cloud.search" connectionString="#{search}#" xdt:Transform="Replace"/>
  <add name="appinsights.instrumentationkey" connectionString="#{aikey}#" xdt:Transform="Replace"/>
</connectionStrings>
