<?xml version="1.0" ?>
<configuration  xmlns:patch="http://www.sitecore.net/xmlconfig/">
  
  <sitecore>

    <settings>
      <setting name="HtmlEditor.DefaultProfile">
        <patch:attribute name="value">/sitecore/system/Settings/Html Editor Profiles/HMPPS Rich Text Custom Profile</patch:attribute>
      </setting>
    </settings>
    
    <pipelines>
      <saveRichTextContent>
        <processor type="HMPPS.Utilities.Pipelines.RichTextEditorAddParagraphs, HMPPS.Utilities" />
      </saveRichTextContent>
    </pipelines>
    
  </sitecore>
  
</configuration>
